{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.99.0",
  "pnpmVersion": "7.32.2",
  "nodeSupportedVersionRange": "^18.12.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "ensureConsistentVersions": true,
  "projects": [
    {
      "packageName": "isTrue",
      "projectFolder": "packages/isTrue",
      "shouldPublish": true,
      "versionPolicyName": "prerelease-monorepo-lockStep"
    },
    {
      "packageName": "app",
      "projectFolder": "packages/app",
      "shouldPublish": true,
      "versionPolicyName": "prerelease-monorepo-lockStep"
    }
  ]
}
